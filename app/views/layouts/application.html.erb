<!DOCTYPE html>
<html>
<head>
  <title>BookmakerFinal</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link href='https://fonts.googleapis.com/css?family=Raleway:400,600,200' rel='stylesheet' type='text/css'>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'common/header' unless is_home_page %>
<div class="<%= "container" unless is_home_page %>">
  <%= flash_messages_tag %>
  <%= yield %>
</div>
<%= render 'common/footer' %>
</body>
</html>
